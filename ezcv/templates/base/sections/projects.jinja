<!-- Projects Section-->
<h2>Projects</h2>
<div>
    {% for project in projects %}
        <!-- Metadata -->
        <h4>{{ project[0]['title'] }}</h4>
        {% if project[0]["image"] %}
        <img src="images/{{ project[0]['image'] }}" alt="{{ project[0]['title'] }}"/>
        {% endif %}

        <!-- Content -->
        <p>{{ project[1]|striptags }}</p>
    {% endfor %}
</div>
<!--End of Projects section-->